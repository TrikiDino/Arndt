<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tvMass"
        android:layout_width="40dp"
        android:layout_height="19dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvAbm"
        android:layout_width="40dp"
        android:layout_height="19dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@+id/tvMass"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/tvVerp"
        android:layout_width="40dp"
        android:layout_height="19dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@+id/tvAbm"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvGew"
        android:layout_width="40dp"
        android:layout_height="19dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@+id/tvVerp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvNo1"
        android:layout_width="120dp"
        android:layout_height="19dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@+id/tvGew"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvNo2"
        android:layout_width="120dp"
        android:layout_height="19dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@+id/tvNo1"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>